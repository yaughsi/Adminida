package AdminLib;

class NodeBookReports {
    String date, mostBorrowedBook;
    int totalBorrowedBooks;
    double totalFine;
    NodeBookReports next;

    public NodeBookReports(String date, String mostBorrowedBook,int totalBorrowedBooks, double totalFine) {
        this.date = date;
        this.mostBorrowedBook = mostBorrowedBook;
        this.totalBorrowedBooks = totalBorrowedBooks;
        this.totalFine = totalFine;
        this.next = null;
    }
}
